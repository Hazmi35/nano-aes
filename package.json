    "lint:fix": "pnpm run lint -- --fix",
    "release": "pnpm publish --access public --no-git-checks"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^13.3.1",
    "@swc/core": "^1.4.4",
    "@tsconfig/node-lts": "^20.1.1",
    "@types/node": "^20.11.24",
    "terser": "^5.28.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.x"
  }
