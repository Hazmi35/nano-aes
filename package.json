{
  "name": "nano-aes",
  "version": "1.1.0",
  "description": "Simple & lightweight AES (Advanced Encryption Standard) module. Wrapper for Node.js crypto module",
  "keywords": [
    "nano-aes",
    "aes",
    "nano aes",
    "encryption",
    "encrypt",
    "decrypt",
    "nodejs"
  ],
  "homepage": "https://github.com/Hazmi35/nano-aes#readme",
  "bugs": {
    "url": "https://github.com/Hazmi35/nano-aes/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hazmi35/nano-aes.git"
  },
  "license": "MIT",
  "author": "Hazmi35 <contact@hzmi.xyz>",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run lint && pnpm run compile",
    "compile": "tsup",
    "lint": "eslint . --ignore-path .gitignore --ext .ts",
    "lint:fix": "pnpm run lint -- --fix",
    "release": "pnpm publish --access public --no-git-checks"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.eslint.json"
    },
    "extends": "@hazmi35/eslint-config/typescript/node",
    "ignorePatterns": "dist/*"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^11.0.0",
    "@swc/core": "^1.3.100",
    "@types/node": "^20.10.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "terser": "^5.25.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.x"
  }
}
